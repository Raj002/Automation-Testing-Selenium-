package Project_Uses;

import java.util.Arrays;
import java.util.Collections;
import java.util.List;
import java.util.Random;

public class Generate_Names 
{
	public static void main(String[] args) 
	{
		String people[]={"ram","kevin","john","kalai","peter","ravi","robin","roman","mohan","david","carol","vivek","rozer","mathi","vishnu","naveen","prithiv","yadhav","vimal"};
		
		List<String> names=Arrays.asList(people);
		Random r=new Random();
		int index=r.nextInt(names.size());
		String anyname=names.get(index);
	    System.out.println(anyname);
	    
	}
}
